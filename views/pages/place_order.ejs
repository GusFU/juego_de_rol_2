                    <div class="campo">
                        <label>Municipio</label>
                        <input id="nombre_origen" type="text" placeholder="Introduce tu Nombre">
                    </div>
                </div>
            </fieldset>
        </div>


        <form action="/place_order" method="POST">
            <div class="formulario" id="form">
                <legend>PLAZE AN ORDER</legend>

                <div class="contenedor">

                    <div class="campo">
                        <label>Peso (kg)</label>
                        <input id="peso" placeholder="Ej: 5" required>
                    </div>

                    <div class="campo">
                        <label>Alto (cm)</label>
                        <input id="alto" placeholder="Ej: 60" required>
                    </div>

                    <div class="campo">
                        <label>Largo (cm)</label>
                        <input id="largo" type="mail" placeholder="Ej: 60" required>
                    </div>

                    <div class="campo">
                        <label>Ancho (cm)</label>
                        <input id="tel" placeholder="Ej: 30" required>
                    </div>



                </div>

                <div class="enviar">
                    <button type="submit" class="boton">Enviar</button>
                    <br><br>
                </div>


        </form>

